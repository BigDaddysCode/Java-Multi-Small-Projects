package functions;
//factorial of a number
import java.util.*;
public class functions08 {
    public static long factorial(int f){
        long fact=1;
        for(int i=1;i<=f;i++){
            fact=fact*i;
        }
        return fact;
    }
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int num= sc.nextInt();
        long fact1=factorial(num);
        System.out.println("Factorial of the given number is "+ fact1);
        System.out.println("Factorial of the given number is "+ factorial(num));
    }
}
